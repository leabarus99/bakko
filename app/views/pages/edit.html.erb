<%= render 'shared/navbar' %>
<%= render 'shared/topbar' %>

 <%= simple_form_for [@user] do |f| %>
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :username %>
    <%= f.input :phone_number %>
    <%= f.input :photo, as: :file, label: "Add a picture"%>
    <%= f.submit %>
  <% end %>
